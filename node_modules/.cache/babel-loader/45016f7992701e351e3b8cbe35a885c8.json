Ac;AACb,cAAA,IAAI,EAAE,MADO;AAEb,cAAA,OAAO,EAAE,QAAO,GAAI,GAAX,GAAiB;AAFb,aAAd;AAID;AACA,SAXD;AAYD,OAdA,MAcO;AACN,aAAK,SAAL,CAAe,QAAQ,CAAC,KAAD,CAAvB,EAAgC,MAAhC,GAAyC,IAAzC;;AACA,QAAA,YAAY,CAAC;AACZ,UAAA,GAAG,EAAE,KAAK,GADE;AAEZ,UAAA,GAAG,EAAE,KAAK,SAAL,CAAe,QAAQ,CAAC,KAAD,CAAvB,EAAgC;AAFzB,SAAD,CAAZ,CAGG,IAHH,CAGQ,UAAC,QAAD,EAAc;AACrB,cAAI,QAAQ,CAAC,IAAT,CAAc,IAAd,KAAuB,GAA3B,EAAgC;AAC/B,YAAA,OAAO,CAAC,GAAR,CAAY,MAAZ;;AACA,YAAA,MAAI,CAAC,QAAL,CAAc;AACb,cAAA,IAAI,EAAE,SADO;AAEb,cAAA,OAAO,EAAE,QAAO,GAAI,GAAX,GAAiB;AAFb,aAAd;AAID;AACA,SAXD;AAYD;AACA;AArEO;AApCK,CAAf","sourcesContent":["<template>\n\t<div id=\"songs-hotmusic\">\n\t\t<!-- :header-cell-style=\"{ fontWeight: 'normal' }\"设置表头样式，中间的连接符去掉，改成驼峰形式 -->\n\t\t<el-table\n\t\t\t:data=\"songsData\"\n\t\t\tstripe\n\t\t\tstyle=\"width: 100%;\"\n\t\t\t:header-cell-style=\"{ fontWeight: 'normal', border: 'none' }\"\n\t\t>\n\t\t\t<el-table-column align=\"center\" prop=\"index\" label=\"\" width=\"120\">\n\t\t\t\t<template #default=\"scope\">\n\t\t\t\t\t<div :class=\"scope.row.downUrl == 'None' ? 'fail' : 'succ'\">\n\t\t\t\t\t\t<sapn\n\t\t\t\t\t\t\tstyle=\"font-size:16px;width:20px;display:inline-block;\"\n\t\t\t\t\t\t>\n\t\t\t\t\t\t\t{{ scope.$index + 1 }}\n\t\t\t\t\t\t</sapn>\n\t\t\t\t\t\t<i\n\t\t\t\t\t\t\tclass=\"iconfont icon-bofang\"\n\t\t\t\t\t\t\tstyle=\"margin-left:10px;font-size:18px;width:20px;display:inline-block;\"\n\t\t\t\t\t\t\t@click=\"startPlay(scope.$index)\"\n\t\t\t\t\t\t\t:class=\"\n\t\t\t\t\t\t\t\tscope.row.downUrl == 'None' ? 'fail' : 'succ'\n\t\t\t\t\t\t\t\"\n\t\t\t\t\t\t></i>\n\t\t\t\t\t\t<i\n\t\t\t\t\t\t\tclass=\"iconfont icon-shoucang_huaban1\"\n\t\t\t\t\t\t\tstyle=\"margin-left:10px;font-size:18px;width:20px;display:inline-block;\"\n\t\t\t\t\t\t\t@click=\"addLove(scope.$index)\"\n\t\t\t\t\t\t\t:class=\"[\n\t\t\t\t\t\t\t\tscope.row.downUrl == 'None' ? 'fail' : 'succ',\n\t\t\t\t\t\t\t\tscope.row.isLove === true ? 'active' : '',\n\t\t\t\t\t\t\t]\"\n\t\t\t\t\t\t></i>\n\t\t\t\t\t</div>\n\t\t\t\t</template>\n\t\t\t</el-table-column>\n\t\t\t<el-table-column\n\t\t\t\talign=\"center\"\n\t\t\t\tprop=\"songName\"\n\t\t\t\tlabel=\"歌曲名\"\n\t\t\t\twidth=\"220\"\n\t\t\t>\n\t\t\t\t<!-- 在表格中添加跳转链接 -->\n\t\t\t\t<template #default=\"scope\">\n\t\t\t\t\t<!-- <el-link underline=\"False\" type=\"primary\" to=\"/\"> -->\n\t\t\t\t\t<span\n\t\t\t\t\t\t:class=\"scope.row.downUrl == 'None' ? 'fail' : 'succ'\"\n\t\t\t\t\t>\n\t\t\t\t\t\t{{ scope.row.songName }}\n\t\t\t\t\t</span>\n\t\t\t\t\t<!-- </el-link> -->\n\t\t\t\t</template>\n\t\t\t</el-table-column>\n\t\t\t<el-table-column\n\t\t\t\talign=\"center\"\n\t\t\t\tprop=\"singerName\"\n\t\t\t\tlabel=\"歌手\"\n\t\t\t\twidth=\"220\"\n\t\t\t>\n\t\t\t\t<template #default=\"scope\">\n\t\t\t\t\t<!-- <el-link underline=\"False\" type=\"primary\" to=\"/\"> -->\n\t\t\t\t\t<span\n\t\t\t\t\t\t:class=\"scope.row.downUrl == 'None' ? 'fail' : 'succ'\"\n\t\t\t\t\t>\n\t\t\t\t\t\t{{ scope.row.singerName }}\n\t\t\t\t\t</span>\n\t\t\t\t\t<!-- </el-link> -->\n\t\t\t\t</template>\n\t\t\t</el-table-column>\n\t\t\t<el-table-column\n\t\t\t\talign=\"center\"\n\t\t\t\tprop=\"album\"\n\t\t\t\tlabel=\"专辑\"\n\t\t\t\twidth=\"300\"\n\t\t\t>\n\t\t\t\t<template #default=\"scope\">\n\t\t\t\t\t<span\n\t\t\t\t\t\t:class=\"scope.row.downUrl == 'None' ? 'fail' : 'succ'\"\n\t\t\t\t\t>\n\t\t\t\t\t\t{{ scope.row.album }}\n\t\t\t\t\t</span>\n\t\t\t\t</template>\n\t\t\t</el-table-column>\n\t\t\t<el-table-column\n\t\t\t\talign=\"center\"\n\t\t\t\tprop=\"songTime\"\n\t\t\t\tlabel=\"时长\"\n\t\t\t\twidth=\"200\"\n\t\t\t>\n\t\t\t\t<template #default=\"scope\">\n\t\t\t\t\t<span\n\t\t\t\t\t\t:class=\"scope.row.downUrl == 'None' ? 'fail' : 'succ'\"\n\t\t\t\t\t>\n\t\t\t\t\t\t{{ scope.row.songTime }}\n\t\t\t\t\t</span>\n\t\t\t\t</template>\n\t\t\t</el-table-column>\n\t\t</el-table>\n\t\t<!-- 分页 -->\n\t\t<el-pagination\n\t\t\tbackground\n\t\t\tlayout=\"prev, pager, next\"\n\t\t\t:total=\"totalNum\"\n\t\t\t:current-page=\"currentPage\"\n\t\t\tstyle=\"padding-top:30px;\"\n\t\t\t@current-change=\"handleCurrentChange\"\n\t\t></el-pagination>\n\t</div>\n</template>\n<script>\nimport { getHotSongs, _addLoveSong,_cancelLoveSong } from \"../network/request.js\"\nimport { useStore } from \"vuex\"\nexport default {\n\tdata() {\n\t\tconst item = {\n\t\t\tiid: \"\",\n\t\t\tsongName: \"\",\n\t\t\tsingerName: \"\",\n\t\t\talbum: \"\",\n\t\t\tsongTime: \"\",\n\t\t\tdownUrl: \"\",\n\t\t\tpicUrl: \"\",\n\t\t}\n\t\treturn {\n\t\t\tuid: \"\",\n\t\t\tdownUrl: \"\",\n\t\t\t// 当前页面\n\t\t\tcurrentPage: 1,\n\t\t\tpageSize: 10,\n\t\t\t// 后台的所有数据\n\t\t\tsongsData: Array(10).fill(item),\n\t\t\t// 表格中渲染的数据\n\t\t\t// tableData: [],\n\t\t\t// 分页中的总条目数\n\t\t\ttotalNum: 100,\n\t\t}\n\t},\n\tcreated() {\n\t\t// 获得用户id\n\t\tthis.uid = useStore().state.user.uid\n\t\tconsole.log(\"用户id：\" + this.uid)\n\t\tconsole.log(this.currentPage)\n\t\t// 得到歌曲的详情信息\n\t\tgetHotSongs({ currentPage: \"1\" }).then((response) => {\n\t\t\tthis.songsData = response.data.data\n\t\t\tconsole.log(this.songsData)\n\t\t})\n\t},\n\tmethods: {\n\t\tgetDiscoverSongsData() {\n\t\t\tgetHotSongs({ currentPage: this.currentPage + \"\" }).then(\n\t\t\t\t(response) => {\n\t\t\t\t\tthis.songsData = response.data.data\n\t\t\t\t\tconsole.log(this.songsData)\n\t\t\t\t},\n\t\t\t)\n\t\t},\n\t\tstartPlay(index) {\n\t\t\tconsole.log(index)\n\t\t\tvar song = this.songsData[parseInt(index)]\n\t\t\tconsole.log(song)\n\t\t\tthis.downUrl = song.downUrl\n\t\t\tconsole.log(this.downUrl)\n\t\t\tconsole.log(song.picUrl)\n\t\t\tvar iid = song.iid\n\t\t\tvar picUrl = song.picUrl\n\t\t\tvar songName = song.songName\n\t\t\tvar singerName = song.singerName\n\t\t\tvar mp3Url = this.downUrl\n\t\t\t// 将事件传递给子组件，第一个参数为自定义事件名，后续参数为传递过去的数据\n\t\t\tconsole.log(\"开始emit\")\n\t\t\tthis.$emit(\"playmusic\", iid, mp3Url, picUrl, songName, singerName)\n\t\t\tconsole.log(\"结束emit\")\n\t\t\t// this.$router.push({ name: \"main\", params: { mp3: \"mp3\" } })\n\t\t\t// this.$parent.audioIsChange = true\n\t\t},\n\t\t// 下一页\n\t\thandleCurrentChange(val) {\n\t\t\t// 得到歌曲的详情信息\n\t\t\tgetHotSongs({ currentPage: val }).then((response) => {\n\t\t\t\tthis.songsData = response.data.data\n\t\t\t\tconsole.log(this.songsData)\n\t\t\t})\n\t\t},\n\t\t// 添加收藏歌曲\n\t\taddLove(index) {\n\t\t\tvar isLove = this.songsData[parseInt(index)].isLove\n\t\t\tvar songName = this.songsData[parseInt(index)].songName\n\t\t\tif (isLove === true) {\n\t\t\t\tthis.songsData[parseInt(index)].isLove = false\n\t\t\t\t_cancelLoveSong({\n\t\t\t\t\tuid: this.uid,\n\t\t\t\t\tiid: this.songsData[parseInt(index)].iid,\n\t\t\t\t}).then((response) => {\n\t\t\t\t\tif (response.data.code === 200) {\n\t\t\t\t\t\tconsole.log(\"取消收藏\")\n\t\t\t\t\t\tthis.$message({\n\t\t\t\t\t\t\ttype: \"info\",\n\t\t\t\t\t\t\tmessage: songName + \" \" + \"取消收藏!\",\n\t\t\t\t\t\t})\n\t\t\t\t\t}\n\t\t\t\t})\n\t\t\t} else {\n\t\t\t\tthis.songsData[parseInt(index)].isLove = true\n\t\t\t\t_addLoveSong({\n\t\t\t\t\tuid: this.uid,\n\t\t\t\t\tiid: this.songsData[parseInt(index)].iid,\n\t\t\t\t}).then((response) => {\n\t\t\t\t\tif (response.data.code === 200) {\n\t\t\t\t\t\tconsole.log(\"收藏歌曲\")\n\t\t\t\t\t\tthis.$message({\n\t\t\t\t\t\t\ttype: \"success\",\n\t\t\t\t\t\t\tmessage: songName + \" \" + \"收藏成功!\",\n\t\t\t\t\t\t})\n\t\t\t\t\t}\n\t\t\t\t})\n\t\t\t}\n\t\t},\n\t},\n}\n</script>\n<style scoped>\n#songs-hotmusic {\n\tposition: absolute;\n\twidth: 1060px !important;\n\tright: 0;\n\tleft: 0;\n\tmargin-left: auto;\n\tmargin-right: auto;\n}\n\n/* 表格中不可播放的的禁止点击样式 */\n#songs-hotmusic .fail {\n\t/* pointer-events: none; */\n\tcolor: #9b9b9b;\n\tcursor: not-allowed;\n}\n/* 可以播放则不做任何修改 */\n#songs-hotmusic .succ {\n\t/* pointer-events: none; */\n\t/* color: #9b9b9b;\n\tcursor: not-allowed; */\n}\n\n/* 收藏歌曲激活状态 */\n#songs-hotmusic .active {\n\tcolor: #f56c6c;\n}\n\n/* 字体图标鼠标覆盖变色 */\n#songs-hotmusic i.iconfont.icon-bofang:hover {\n\tcursor: pointer;\n\t/* color: rgb(231, 56, 40); */\n\tcolor: #f56c6c;\n}\n\n/* 字体图标鼠标覆盖变色 */\n#songs-hotmusic i.iconfont.icon-shoucang_huaban1:hover {\n\tcursor: pointer;\n}\n</style>\n"],"sourceRoot":""}]}