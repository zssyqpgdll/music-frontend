cope.$index)\"\n\t\t\t\t\t\t\t:class=\"\n\t\t\t\t\t\t\t\tscope.row.downUrl == 'None' ? 'fail' : 'succ'\n\t\t\t\t\t\t\t\"\n\t\t\t\t\t\t></i>\n\t\t\t\t\t\t<i\n\t\t\t\t\t\t\tclass=\"iconfont icon-shoucang_huaban1\"\n\t\t\t\t\t\t\tstyle=\"margin-left:10px;font-size:18px;width:20px;display:inline-block;\"\n\t\t\t\t\t\t\t@click=\"deleteLove(scope.$index)\"\n\t\t\t\t\t\t\t:class=\"[\n\t\t\t\t\t\t\t\tscope.row.downUrl == 'None' ? 'fail' : 'succ',\n\t\t\t\t\t\t\t\tscope.row.isLove === true ? 'active' : '',\n\t\t\t\t\t\t\t]\"\n\t\t\t\t\t\t></i>\n\t\t\t\t\t</div>\n\t\t\t\t</template>\n\t\t\t</el-table-column>\n\t\t\t<el-table-column\n\t\t\t\talign=\"center\"\n\t\t\t\tprop=\"songName\"\n\t\t\t\tlabel=\"歌曲名\"\n\t\t\t\twidth=\"220\"\n\t\t\t>\n\t\t\t\t<!-- 在表格中添加跳转链接 -->\n\t\t\t\t<template #default=\"scope\">\n\t\t\t\t\t<!-- <el-link underline=\"False\" type=\"primary\" to=\"/\"> -->\n\t\t\t\t\t<span\n\t\t\t\t\t\t:class=\"scope.row.downUrl == 'None' ? 'fail' : 'succ'\"\n\t\t\t\t\t>\n\t\t\t\t\t\t{{ scope.row.songName }}\n\t\t\t\t\t</span>\n\t\t\t\t\t<!-- </el-link> -->\n\t\t\t\t</template>\n\t\t\t</el-table-column>\n\t\t\t<el-table-column\n\t\t\t\talign=\"center\"\n\t\t\t\tprop=\"singerName\"\n\t\t\t\tlabel=\"歌手\"\n\t\t\t\twidth=\"220\"\n\t\t\t>\n\t\t\t\t<template #default=\"scope\">\n\t\t\t\t\t<!-- <el-link underline=\"False\" type=\"primary\" to=\"/\"> -->\n\t\t\t\t\t<span\n\t\t\t\t\t\t:class=\"scope.row.downUrl == 'None' ? 'fail' : 'succ'\"\n\t\t\t\t\t>\n\t\t\t\t\t\t{{ scope.row.singerName }}\n\t\t\t\t\t</span>\n\t\t\t\t\t<!-- </el-link> -->\n\t\t\t\t</template>\n\t\t\t</el-table-column>\n\t\t\t<el-table-column\n\t\t\t\talign=\"center\"\n\t\t\t\tprop=\"album\"\n\t\t\t\tlabel=\"专辑\"\n\t\t\t\twidth=\"300\"\n\t\t\t>\n\t\t\t\t<template #default=\"scope\">\n\t\t\t\t\t<span\n\t\t\t\t\t\t:class=\"scope.row.downUrl == 'None' ? 'fail' : 'succ'\"\n\t\t\t\t\t>\n\t\t\t\t\t\t{{ scope.row.album }}\n\t\t\t\t\t</span>\n\t\t\t\t</template>\n\t\t\t</el-table-column>\n\t\t\t<el-table-column\n\t\t\t\talign=\"center\"\n\t\t\t\tprop=\"songTime\"\n\t\t\t\tlabel=\"时长\"\n\t\t\t\twidth=\"200\"\n\t\t\t>\n\t\t\t\t<template #default=\"scope\">\n\t\t\t\t\t<span\n\t\t\t\t\t\t:class=\"scope.row.downUrl == 'None' ? 'fail' : 'succ'\"\n\t\t\t\t\t>\n\t\t\t\t\t\t{{ scope.row.songTime }}\n\t\t\t\t\t</span>\n\t\t\t\t</template>\n\t\t\t</el-table-column>\n\t\t</el-table>\n\t\t<!-- 分页 -->\n\t\t<!-- <el-pagination\n\t\t\tbackground\n\t\t\tlayout=\"prev, pager, next\"\n\t\t\t:total=\"totalNum\"\n\t\t\t:current-page=\"currentPage\"\n\t\t\tstyle=\"padding-top:30px;\"\n\t\t\t@current-change=\"handleCurrentChange\"\n\t\t></el-pagination> -->\n\t</div>\n</template>\n\n<script>\nimport {\n\t_getLoveSongList,\n\t_addLoveSong,\n\t_cancelLoveSong,\n} from \"../network/request.js\"\nimport { useStore } from \"vuex\"\nexport default {\n\tname: \"love-song\",\n\tdata() {\n\t\tconst item = {\n\t\t\tiid: \"\",\n\t\t\tsongName: \"\",\n\t\t\tsingerName: \"\",\n\t\t\talbum: \"\",\n\t\t\tsongTime: \"\",\n\t\t\tdownUrl: \"\",\n\t\t\tpicUrl: \"\",\n\t\t\t// 收藏列表默认为收藏\n\t\t\tisLove: true,\n\t\t}\n\t\treturn {\n\t\t\tuid: \"\",\n\t\t\tdownUrl: \"\",\n\t\t\t// 当前页面\n\t\t\t// currentPage: 1,\n\t\t\t// pageSize: 10,\n\t\t\t// 后台的所有数据\n\t\t\tsongsData: Array(10).fill(item),\n\t\t\t// 表格中渲染的数据\n\t\t\t// tableData: [],\n\t\t\t// 分页中的总条目数\n\t\t\t// totalNum: 1000,\n\t\t}\n\t},\n\tcreated() {\n\t\t// 获得用户id\n\t\tthis.uid = useStore().state.user.uid\n\t\tconsole.log(\"用户id：\" + this.uid)\n\t\t// console.log(this.currentPage)\n\t\t// 得到歌曲的详情信息\n\t\t_getLoveSongList({ uid: this.uid }).then((response) => {\n\t\t\tthis.songsData = response.data.data\n\t\t\tconsole.log(this.songsData)\n\t\t\tfor (var i = 0; i < this.songsData.length; i++) {\n\t\t\t\tthis.songsData[i].isLove = true\n\t\t\t}\n\t\t})\n        // 页面加载时读取数据\n\t\tif (window.sessionStorage.getItem(\"user\")) {\n\t\t\tconsole.log(\"页面加载\")\n            const session_user = JSON.parse(window.sessionStorage.getItem(\"user\"))\n\t\t\tconsole.log(session_user)\n\t\t\tthis.$store.commit(\"SET_USER\", session_user)\n\t\t}\n\t\t// 页面刷新时保存状态\n\t\twindow.addEventListener(\"beforeunload\", () => {\n\t\t\tconsole.log(\"页面刷新\")\n\t\t\tconsole.log(this.$store.state.user)\n\t\t\twindow.sessionStorage.setItem(\n\t\t\t\t\"user\",\n\t\t\t\tJSON.stringify(this.$store.state.user),\n\t\t\t)\n\t\t})\n\t},\n\tmethods: {\n\t\tstartPlay(index) {\n\t\t\tconsole.log(index)\n\t\t\tvar song = this.songsData[parseInt(index)]\n\t\t\tconsole.log(song)\n\t\t\tthis.downUrl = song.downUrl\n\t\t\tconsole.log(this.downUrl)\n\t\t\tconsole.log(song.picUrl)\n\t\t\tvar iid = song.iid\n\t\t\tvar picUrl = song.picUrl\n\t\t\tvar songName = song.songName\n\t\t\tvar singerName = song.singerName\n\t\t\tvar mp3Url = this.downUrl\n\t\t\t// 将事件传递给子组件，第一个参数为自定义事件名，后续参数为传递过去的数据\n\t\t\tconsole.log(\"开始emit\")\n\t\t\tthis.$emit(\"playmusic\", iid, mp3Url, picUrl, songName, singerName)\n\t\t\tconsole.log(\"结束emit\")\n\t\t},\n\t\t// 下一页\n\t\t// handleCurrentChange(val) {\n\t\t// \t// 得到歌曲的详情信息\n\t\t// \tgetDiscoverSongs({ currentPage: val }).then((response) => {\n\t\t// \t\tthis.songsData = response.data.data\n\t\t// \t\tconsole.log(this.songsData)\n\t\t// \t})\n\t\t// },\n\t\t// 取消收藏歌曲\n\t\tdeleteLove(index) {\n\t\t\tvar isLove = this.songsData[parseInt(index)].isLove\n\t\t\tvar songName = this.songsData[parseInt(index)].songName\n\t\t\tif (isLove === true) {\n\t\t\t\tthis.songsData[parseInt(index)].isLove = false\n\t\t\t\t_cancelLoveSong({\n\t\t\t\t\tuid: this.uid,\n\t\t\t\t\tiid: this.songsData[parseInt(index)].iid,\n\t\t\t\t}).then((response) => {\n\t\t\t\t\tif (response.data.code === 200) {\n\t\t\t\t\t\tconsole.log(\"取消收藏\")\n\t\t\t\t\t\tthis.$message({\n\t\t\t\t\t\t\ttype: \"info\",\n\t\t\t\t\t\t\tmessage: songName + \" \" + \"取消收藏!\",\n\t\t\t\t\t\t})\n\t\t\t\t\t}\n\t\t\t\t})\n\t\t\t} else {\n\t\t\t\tthis.songsData[parseInt(index)].isLove = true\n\t\t\t\t_addLoveSong({\n\t\t\t\t\tuid: this.uid,\n\t\t\t\t\tiid: this.songsData[parseInt(index)].iid,\n\t\t\t\t}).then((response) => {\n\t\t\t\t\tif (response.data.code === 200) {\n\t\t\t\t\t\tconsole.log(\"收藏歌曲\")\n\t\t\t\t\t\tthis.$message({\n\t\t\t\t\t\t\ttype: \"success\",\n\t\t\t\t\t\t\tmessage: songName + \" \" + \"收藏成功!\",\n\t\t\t\t\t\t})\n\t\t\t\t\t}\n\t\t\t\t})\n\t\t\t}\n\t\t},\n\t},\n}\n</script>\n<style scoped>\n#love-song {\n\tposition: absolute;\n\twidth: 1060px !important;\n\tright: 0;\n\tleft: 0;\n\tmargin-left: auto;\n\tmargin-right: auto;\n}\n\n/* 表格中不可播放的的禁止点击样式 */\n#love-song .fail {\n\t/* pointer-events: none; */\n\tcolor: #9b9b9b;\n\tcursor: not-allowed;\n}\n/* 可以播放则不做任何修改 */\n#love-song .succ {\n\t/* pointer-events: none; */\n\t/* color: #9b9b9b;\n\tcursor: not-allowed; */\n}\n/* 收藏歌曲激活状态 */\n#love-song .active {\n\tcolor: #f56c6c;\n}\n\n/* 字体图标鼠标覆盖变色 */\n#love-song i.iconfont.icon-bofang:hover {\n\tcursor: pointer;\n\t/* color: rgb(231, 56, 40); */\n\tcolor: #f56c6c;\n}\n\n/* 字体图标鼠标覆盖变色 */\n#love-song i.iconfont.icon-shoucang_huaban1:hover {\n\tcursor: pointer;\n}\n</style>\n"],"sourceRoot":""}]}